import javax.swing.JOptionPane;

   public class Antecessor{
	   public static void main(String[] args){
         
         int nun = Integer.parseInt(JOptionPane.showInputDialog("Informe um numero: "));
         
         switch (nun){
            
            case 1: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Domingo");

               break;
               
             case 2: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Segunda");

               break;


             case 3: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Terça");

               break;


             case 4: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Quarta");

               break;


             case 5: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Quinta");

               break;


             case 6: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Sexta");

               break;


             case 7: 
               
                  JOptionPane.showMessageDialog(null,"O numero corresponde a Sábado");

               break;    
         
         }
      	
	}
}
